<!-- Ann_Lamp -->


 <animation>
  <object-name>chipsInOil.Light</object-name>
  <type>select</type>
  <condition>
  <or>
  <and>
  <greater-than>
    <property>/systems/electrical/outputs/bus</property>
    <value>21</value>
