<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>Carb.ac</path>

  <animation>
    <type>noshadow</type>
    <object-name>Glass</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>glass.png</texture>
    <object-name>Glass</object-name>
  </animation>


 <animation>
  <type>rotate</type>
  <object-name>Needle</object-name>  
  <property>/engines/engine/cht-degf</property>
       <interpolation>
        <entry><ind> 32</ind><dep> 21</dep></entry>
        <entry><ind> 1500</ind><dep> 1</dep></entry>
        <entry><ind> 1800</ind><dep> -2</dep></entry>
       </interpolation>
  <center>     
   <x-m>0.0</x-m>
   <y-m>0.00</y-m>
   <z-m>-0.0085</z-m>
  </center>
  <axis>
   <x>1.0</x>
   <y>0.0</y>
   <z>0.0</z>
  </axis>
</animation>

</PropertyList>
