<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>Fuel.ac</path>


  <animation>
    <type>noshadow</type>
    <object-name>Glass</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>glass.png</texture>
    <object-name>Glass</object-name>
  </animation>
 

  <animation> 
    <type>rotate</type>
    <object-name>Needle</object-name>
    <property>consumables/fuel/tank[0]/level-lbs</property>
    <interpolation>
     <entry><ind>  0.0 </ind><dep>44.0 </dep></entry>
     <entry><ind> 315.0 </ind><dep>-44.0 </dep></entry>
    </interpolation>
    <center>
      <x-m>  0.00 </x-m>
      <y-m>  0.00 </y-m>
      <z-m> -0.021 </z-m>  
    </center>
    <axis> 
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>         
 

</PropertyList>
