<?xml version="1.0"?>

<PropertyList>

  <path>RPM.ac</path>

  <animation>
    <type>noshadow</type>
    <object-name>Glass</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>glass.png</texture>
    <object-name>Glass</object-name>
  </animation>


	<animation>
		<type>rotate</type>
		<object-name>Needle</object-name>
  <property>/engines/engine[0]/rpm</property>
       <interpolation>
        <entry><ind> 0</ind><dep>  315</dep></entry>
        <entry><ind> 2950</ind><dep>  0</dep></entry>
       </interpolation>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

</PropertyList> 
