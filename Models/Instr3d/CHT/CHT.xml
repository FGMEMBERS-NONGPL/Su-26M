<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>CHT.ac</path>

  <animation>
    <type>noshadow</type>
    <object-name>Glass</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>glass.png</texture>
    <object-name>Glass</object-name>
  </animation>


 <animation>
  <type>rotate</type>
  <object-name>Needle</object-name>  
  <property>/engines/engine/cht-degf</property>
       <interpolation>
        <entry><ind> 32</ind><dep> -80</dep></entry>
        <entry><ind> 450</ind><dep> -155</dep></entry>
        <entry><ind> 1650</ind><dep> -220</dep></entry>
        <entry><ind> 1800</ind><dep> -250</dep></entry>
       </interpolation>
  <axis>
   <x>1.0</x>
   <y>0.0</y>
   <z>0.0</z>
  </axis>
</animation>

</PropertyList>


