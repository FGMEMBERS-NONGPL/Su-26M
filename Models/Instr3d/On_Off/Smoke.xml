<?xml version="1.0"?>
<PropertyList>
    <path>Smoke.ac</path>


  <animation>
    <type>pick</type>
    <object-name>onSmoke</object-name>
    <visible>false</visible>
  <action>                             
   <desc>SmokeWings</desc>
   <button>0</button>               
    <binding>
    <command>property-assign</command>
    <property>/control/smokewings</property>
     <value>1</value>                     
   </binding>
  </action>
  </animation>  

  <animation>
    <type>pick</type>
    <object-name>offSmoke</object-name>
    <visible>false</visible>
  <action>                             
   <desc>SmokeWings</desc>
   <button>0</button>               
    <binding>
    <command>property-assign</command>
    <property>/control/smokewings</property>
      <value>0</value>                     
   </binding>
  </action>
  </animation>  
    
 
 <animation>
  <type>rotate</type>
  <object-name>Smoke</object-name>  
  <property>/control/smokewings</property>
  <factor>-40</factor> 
  <center>
   <x-m>0.0029</x-m>
   <y-m>0.0</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>1.0</y>
   <z>0.0</z>
  </axis>
 </animation>
 
          
 
        
</PropertyList>
