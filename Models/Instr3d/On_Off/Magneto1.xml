<?xml version="1.0"?>
<PropertyList>
    <path>Magneto1.ac</path>


  <animation>
    <type>pick</type>
    <object-name>onMag1</object-name>
    <visible>false</visible>
  <action>                             
   <desc>Magneto 1</desc>
   <button>0</button>               
    <binding>
    <command>property-assign</command>
    <property>/controls/engines/engine/magneto1</property>
      <value>1</value>                     
   </binding>
   <binding>
      <command>nasal</command>
      <script>scripts.magneto_switch()</script>
   </binding>      
  </action>
  </animation>  

  <animation>
    <type>pick</type>
    <object-name>offMag1</object-name>
    <visible>false</visible>
  <action>                             
   <desc>Magneto 1</desc>
   <button>0</button>               
    <binding>
    <command>property-assign</command>
    <property>/controls/engines/engine/magneto1</property>
      <value>0</value>                     
   </binding>
   <binding>
      <command>nasal</command>
      <script>scripts.magneto_switch()</script>
   </binding>      
  </action>
  </animation>  
    
 
 <animation>
  <type>rotate</type>
  <object-name>Magneto1</object-name>  
  <property>/controls/engines/engine/magneto1</property>
  <factor>-40</factor> 
  <center>
   <x-m>0.0029</x-m>
   <y-m>0.0</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>1.0</y>
   <z>0.0</z>
  </axis>
 </animation>
 
          
 
        
</PropertyList>
