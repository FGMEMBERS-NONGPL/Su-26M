<?xml version="1.0"?>

<!-- Menu for setting fuel and load -->

<PropertyList>
 <name>Fuel and load</name>
 
   <width>150</width>
   <height>100</height>
   <modal>false</modal>
   <draggable>true</draggable> 
   
   <slider>
    <x>10</x>
    <y>10</y>
    <width>130</width>
    <property>consummables/fuel/tank/level-lb</property>
    <min>0</min>
    <max>315</max>
   </slider>
   
   <textbox>
    <!-- position -->
    <x>10</x>
    <y>50</y>

    <!-- dimensions -->
    <width>130</width>
    <height>30</height>

    <property>consummables/fuel/tank/level-lb</property>

    <editable>true</editable> <!-- whether the puLargeInput is supposed to be editable -->
</textbox>



</PropertyList>
