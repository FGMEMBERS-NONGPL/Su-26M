<PropertyList>

 <name>Su-26 main panel</name>
 <background>Aircraft/Su-26M/Panels/transparent-bg.rgb</background>
 <w>1024</w>
 <h>768</h>
 <!--<view-height>768</view-height>-->
 
<instruments>
  
 <instrument>

  <name>controls</name>
   <x>512</x>
   <y>384</y>   
   <w-base>1024</w-base>
   <h-base>768</h-base>

 <actions>
 
  <action>                             
   <desc>browse leaflet</desc>
   <button>0</button>               
   <x>-70</x>                        
   <y>230</y>
   <w>120</w>                        
   <h>90</h>
   <binding>
    <command>property-toggle</command>
    <property>controls/engines/engine/starter</property>                     
   </binding>
  </action>

  <action>                             
   <desc>battery switch</desc>
   <button>0</button>               
   <x>75</x>                        
   <y>60</y>
   <w>20</w>                        
   <h>20</h>
   <binding>
    <command>property-toggle</command>
    <property>controls/engines/engine/starter</property>                     
   </binding>
  </action>
  
  <action>                             
   <desc>Lamp test</desc>
   <button>0</button>               
   <x>-243</x>                        
   <y>30</y>
   <w>20</w>                        
   <h>20</h>
   <binding>
      <command>property-assign</command>
      <property>controls/lamp-test</property>      
      <value>true</value>                 
   </binding>
   <binding>
      <command>property-assign</command>
      <property>controls/lamp-test-motion</property>      
      <value>-0.002</value>                 
   </binding>   
   <mod-up>
    <binding>
      <command>property-assign</command>
      <property>controls/lamp-test</property>      
      <value>false</value>
    </binding>
    <binding>
      <command>property-assign</command>
      <property>controls/lamp-test-motion</property>      
      <value>0.0</value>   
    </binding>
   </mod-up>      
  </action>
  
  <action>			       
   <desc>shut off valve</desc>
   <button>0</button>		       
   <x>460</x>  			 
   <y>-50</y>
   <w>50</w>			       
   <h>50</h>
   <binding>
    <command>property-assign</command>
    <property>controls/engines/engine/mixture</property>		     
    <value>0.0</value>
   </binding>		 
  </action>
 
  <action>			       
   <desc>generator breaker</desc>
   <button>0</button>		       
   <x>-95</x>  			 
   <y>-115</y>
   <w>20</w>			       
   <h>20</h>
   <binding>
    <command>property-toggle</command>
    <property>controls/engines/engine/starter</property>		     
   </binding>			 
  </action>
  
  <action>			       
   <desc>radio breaker</desc>
   <button>0</button>		       
   <x>-58</x>  			 
   <y>-115</y>
   <w>20</w>			       
   <h>20</h>
   <binding>
    <command>property-toggle</command>
    <property>instruments/comm/serviceable</property>		     
   </binding>			 
  </action>    
 
  <action>			       
   <desc>fire breaker</desc>
   <button>0</button>		       
   <x>-25</x>  			 
   <y>-115</y>
   <w>20</w>			       
   <h>20</h>
   <binding>
    <command>property-toggle</command>
    <property>controls/engines/engine/starter</property>		     
   </binding>			 
  </action>
  
  <action>			       
   <desc>pitot breaker</desc>
   <button>0</button>		       
   <x>8</x>  			 
   <y>-115</y>
   <w>20</w>			       
   <h>20</h>
   <binding>
    <command>property-toggle</command>
    <property>controls/anti-ice/pitot-heat</property>		     
   </binding>			 
  </action>
   
  <action>			       
   <desc>instruments breaker</desc>
   <button>0</button>		       
   <x>42</x>  			 
   <y>-115</y>
   <w>20</w>			       
   <h>20</h>
   <binding>
    <command>property-toggle</command>
    <property>controls/engines/engine/starter</property>		     
   </binding>			 
  </action>
  
  <action>			       
   <desc>battery breaker</desc>
   <button>0</button>		       
   <x>73</x>  			 
   <y>-115</y>
   <w>20</w>			       
   <h>20</h>
   <binding>
    <command>property-toggle</command>
    <property>controls/engines/engine/starter</property>		     
   </binding>			 
  </action> 
  
  <action>			       
   <desc>chronometer start</desc>
   <button>0</button>		       
   <x>-395</x>  			 
   <y>-10</y>
   <w>20</w>			       
   <h>20</h>
   <binding>
    <command>property-toggle</command>
    <property>controls/engines/engine/starter</property>		     
   </binding>			 
  </action>
  
  <action>			       
   <desc>baro setting decrease</desc>
   <button>0</button>		       
   <x>-170</x>  			 
   <y>-80</y>
   <w>20</w>			       
   <h>20</h>
   <binding>
    <command>property-adjust</command>
    <property>instrumentation/altimeter/setting-inhg</property>
    <step>-0.01</step>
    <min>27.00</min>
    <max>32.00</max>
    <wrap>false</wrap>
   </binding>			 
  </action>   
  
  <action>			       
   <desc>baro setting increase</desc>
   <button>0</button>		       
   <x>-150</x>  			 
   <y>-80</y>
   <w>20</w>			       
   <h>20</h>
   <binding>
    <command>property-adjust</command>
    <property>instrumentation/altimeter/setting-inhg</property>
    <step>0.01</step>
    <min>27.00</min>
    <max>32.00</max>
    <wrap>false</wrap>
   </binding>			 
  </action>                
 
  <action>			       
   <desc>Canopy jettison</desc>
   <button>0</button>		       
   <x>-520</x>  			 
   <y>-50</y>
   <w>80</w>			       
   <h>80</h>
   <binding>
    <command>property-assign</command>
    <property>controls/canopy/canopy-jettison</property>		     
    <value>true</value>
   </binding>
   <binding>
    <command>property-assign</command>
    <property>controls/canopy/jettison-handle-motion</property>		     
    <value>0.05</value>
   </binding>		 
  </action>
 
  <action>			       
   <desc>Radio Volume decrease</desc>
   <button>0</button>		       
   <x>-130</x>  			 
   <y>-225</y>
   <w>20</w>			       
   <h>20</h>
   <binding>
    <command>property-adjust</command>
    <property>instrumentation/comm/frequencies/selected-mhz</property>
    <step>-0.05</step>
    <min>0.0</min>
    <max>1.0</max>
    <wrap>false</wrap>
   </binding>			 
  </action>
  
  <action>			       
   <desc>Radio Volume increase</desc>
   <button>0</button>		       
   <x>-110</x>  			 
   <y>-225</y>
   <w>20</w>			       
   <h>20</h>
   <binding>
    <command>property-adjust</command>
    <property>instrumentation/comm/frequencies/selected-mhz</property>
    <step>0.05</step>
    <min>0.0</min>
    <max>1.0</max>
    <wrap>false</wrap>
   </binding>			 
  </action>
  
  <action>			       
   <desc>Squelch</desc>
   <button>0</button>		       
   <x>-120</x>  			 
   <y>-290</y>
   <w>20</w>			       
   <h>20</h>
   <binding>
    <command>property-toggle</command>
    <property>controls/engines/engine/starter</property>		     
   </binding>			 
  </action>  

  <action>			       
   <desc>large frequency decrease</desc>
   <button>0</button>		       
   <x>-80</x>  			 
   <y>-290</y>
   <w>20</w>			       
   <h>20</h>
   <binding>
    <command>property-adjust</command>
    <property>instrumentation/comm/frequencies/selected-mhz</property>
    <mask>integer</mask>
    <step>-1</step>
    <min>118</min>
    <max>136</max>
    <resolution>1</resolution>
    <wrap>true</wrap>
   </binding>		 
  </action>
  
  <action>			       
   <desc>large frequency increase</desc>
   <button>0</button>		       
   <x>-60</x>  			 
   <y>-290</y>
   <w>20</w>			       
   <h>20</h>
   <binding>
    <command>property-adjust</command>
    <property>instrumentation/comm/frequencies/selected-mhz</property>
    <mask>integer</mask>
    <step>1</step>
    <min>118</min>
    <max>136</max>
    <resolution>1</resolution>
    <wrap>true</wrap>
   </binding>			 
  </action>
  
  <action>			       
   <desc>small frequency decrease</desc>
   <button>0</button>		       
   <x>90</x>  			 
   <y>-290</y>
   <w>20</w>			       
   <h>20</h>
   <binding>
    <command>property-adjust</command>
    <property>instrumentation/comm/frequencies/selected-mhz</property>
    <mask>decimal</mask>
    <step>-0.025</step>
    <min>0.000</min>
    <max>1.000</max>
    <resolution>0.025</resolution>
    <wrap>true</wrap>
   </binding>			 
  </action>
  
  <action>			       
   <desc>large frequency increase</desc>
   <button>0</button>		       
   <x>110</x>  			 
   <y>-290</y>
   <w>20</w>			       
   <h>20</h>
   <binding>
    <command>property-adjust</command>
    <property>instrumentation/comm/frequencies/selected-mhz</property>
    <mask>decimal</mask>
    <step>0.025</step>
    <min>0.000</min>
    <max>1.000</max>
    <resolution>0.025</resolution>
    <wrap>true</wrap>
   </binding>		 
  </action>  
    
 </actions>
 
 </instrument>
 
 </instruments> 
   
</PropertyList>
